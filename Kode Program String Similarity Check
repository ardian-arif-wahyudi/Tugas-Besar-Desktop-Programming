    private void string1ActionPerformed(java.awt.event.ActionEvent evt) {                                        
        
        JFileChooser jfc = new JFileChooser();
        jfc.showOpenDialog(null);
        File file = jfc.getSelectedFile();
        String dir = file.getAbsolutePath();
        jTextField1.setText(dir);
      
        
         
    }                                       

    private void string2ActionPerformed(java.awt.event.ActionEvent evt) {                                        
        
       JFileChooser jfc = new JFileChooser();
        jfc.showOpenDialog(null);
        File file2 = jfc.getSelectedFile();
        String dir2 = file2.getAbsolutePath();
        jTextField2.setText(dir2);
       
    }                                       

