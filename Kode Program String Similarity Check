    }                                       

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       
        String file1,file2,s1,s2,s3,s4;
        file1 = jTextField1.getText();
        file2 = jTextField2.getText();
        s1 = bacaFile(file1);
        s2 = bacaFile(file2);
        s3 = string3.getText();
        s4 = string4.getText();
        double similarity = findSimilarity(s1,s2);
        double similarity2 = findSimilarity(s3,s4);
        double hasil = similarity * 100 ;
        double hasil2 = similarity2 * 100 ;
        JOptionPane.showMessageDialog(null, "Presentase Plagiarisme Input File Sebesar " + hasil+"%\nPresentase Plagiarisme Input Manual Sebesar "+hasil2+"%");

    }                                        

